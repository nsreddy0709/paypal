
<meta name="viewport" content="width=device-width, initial-scale=1">

<style>
    body {
        font-family: Arial, Helvetica, sans-serif;
        font-size:2vw;
        background-image:url('data:image/jpeg;base64,');
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-size: cover;
        margin-left11:auto;
        margin-right:auto;
    }
    h1{
        font-size:4vw;
    }
    h2{
        font-size:3vw;
    }

    form {border: 3px solid #f1f1f1;

        background-color:white;
        display: block;
        margin-top: 50px;
        margin-left: auto;
        margin-right: auto;
        width: 40%;
        height:90%; }
    input[type=text], input[type=password] {

        padding: 12px 20px;
        margin: 8px 0;
        display: inline-block;
        border: 1px solid #cccc;
        width:100%
    }

    button:hover {
        opacity: 0.8;
        background-color:#0052cc;
    }
    .next{background-colour:#0066ff;
    }
    .cancelbtn {
        width: auto;
        padding: 10px 18px;
        background-color: #f44336;
    }
    .imgcontainer {
        text-align: center;
        margin: 24px 0 12px 0;
    }
    img.avatar {
        width: 40%;
        border-radius: 50%;
    }
    .logo{
        width:25%;
    }

    .pay{
        text-align:center;
        font-size: 15px;
    }

    .enter{
        text-align:center;
    }
    span.psw {
        float: right;
        padding-top: 16px;
    }
    .center {
        display: block;
        margin-left: auto;
        margin-right: auto;

        font-size:2vw;
    }
    .or{
        text-align:center
    }
    /* Change styles for span and cancel button on extra small screens */
    @media screen and (max-width: 300px) {

        span.psw {
            display: block;
            float: none;

        }
    }

</style>
<body>

<form action="${pageContext.request.contextPath}/pay" method="post">
    <div class="imgcontainer">
        <img src="https://australianliquorsuppliers.com.au/wp-content/uploads/2020/08/Paypal-Logo-1.png" alt="Avatar" class="logo">
    </div>

    <div class="pay">
        <h1>Welcome to paypal payment<br> ${uname}</h1><br>
        <h2>Total amount<br> ${amount}</h2>


    </div>
    <div class="containers center">

        <input type="hidden" name="amount" value=${amount}>
        <input type="hidden" name="uname" value=${uname}>
        <input type="hidden" name="jwt" value=${jwt}>

        <button  type="submit" class="btn center btn-lg btn-md btn-sm btn-sm btn-primary">Pay Now</button>




    </div>


</form>



</body>





